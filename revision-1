#include<stdio.h>
int main()
{
char ch;
    printf("Enter any character: ");
    scanf("%d",&ch);
    if((ch >= 'a' && ch <= 'z') || (ch >= 'A' && ch <= 'Z'))
    {
    printf("'%d is alphabet",ch);
    }
    else if(ch >= '0' && ch <= '9')
    {
        printf("'%d is digit", ch);
    }
    else 
    {
        printf("'%d is special character", ch);
    }
}
